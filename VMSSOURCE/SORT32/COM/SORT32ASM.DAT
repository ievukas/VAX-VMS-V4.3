%SORSPCUTI/SORSPCUTI
%SORSPEC/SORSPEC
%SRTSPCBLI/SRTSPCBLI
%SORFILNAM/SORFILNAM
%SORCOLUTI/SORCOLUTI
%CRETRANS/CRETRANS
!
$ LANG:=BLISS; ML1:=""; ML2:=""; ML3:=""
$ OLB:=SORTMERGE; GBL:=GLOBALS; OPT := /NOTRACE
%SRTTRN/SRTTRN
!
