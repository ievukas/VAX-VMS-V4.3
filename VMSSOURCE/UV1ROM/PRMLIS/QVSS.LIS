                                     0000    28 ;
                                     0000    29 ; Facility: MicroVAX I system bootstrap
                                     0000    30 ;
                                     0000    31 ; Abstract:
                                     0000    32 ;
                                     0000    33 ;	This set of subroutines provides access to the QVSS keyboard and video
                                     0000    34 ;	screen.
                                     0000    35 ;
                                     0000    36 ; Environment:
                                     0000    37 ;
                                     0000    38 ;	These routines assume that the QVSS has been set up properly by the
                                     0000    39 ;	MicroVax I microcode.
                                     0000    40 ;
                                     0000    41 ; Author:
                                     0000    42 ;
                                     0000    43 ;	Len Kawell, D. N. Cutler 28-Dec-83
                                     0000    44 ;
                                     0000    45 ; Modified by:
                                     0000    46 ;
                                     0000    47 ;	V1.0-02 Len Kawell 18-Jun-1984
