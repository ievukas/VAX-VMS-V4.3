$
$OPEN_FILES:
$	OPEN /READ INPUT 'P1'
$	OPEN /WRITE OUTPUT 'P1'
$
$MAIN_LOOP:
$	READ INPUT RECORD /END_OF_FILE=CLOSE_FILES
$	IF RECORD .EQS. "" THEN GOTO WRITE_IT
$	IF F$LOCATE ("FDL$S_FDLDEF", RECORD) .NE. F$LENGTH (RECORD) THEN GOTO MAIN_LOOP
$
$WRITE_IT:
$	WRITE OUTPUT RECORD
