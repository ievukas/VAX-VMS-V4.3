!
! Version:	'V04-000'
!
!	V03-001	RNG0001			Rod Gamache	7-Jun-1984
!		Change severity for error from SEVERE to ERROR
!		Add error message for failure t ocreate history buffer.
!
	.FACILITY LATCP, 3 /PREFIX=LCP$_

	.SEVERITY ERROR

IVCMD		<Invalid command>
IVQUAL/FAO=2 	"Value for qualifier !AS is invalid as !18<'!AS'!>"
NOTLOADED	<LAT terminal port driver (LTDRIVER) is not loaded>
NOTINITED	<LAT terminal port driver controller init not called>
NOTSTARTED	<LAT terminal port driver not started>
NOTSTOPPED	<LAT terminal port driver not stopped>
NOTSET		<LAT terminal port driver multicast data not set>
NOHISTORY	<No history buffer is available>
NOSTARTHIST	<LAT history journalling not started>
NOTFROMLAT	<Unable to shut down LAT from a LAT terminal>
INTERNAL	<LATCP internal error>

	.SEVERITY INFORMATIONAL

STARTED		<LAT terminal port driver started>
STOPPED		<LAT terminal port driver stopped>
SET		<LAT terminal port driver multicast data set>
NOSERVERS	<No known servers>

	.END
