;+
;	MOVTC  --  Move Translated Characters
;
;		srclen.rw	Length of source string
;		srcaddr.ab	Address of source string
;		fill.rb		Fill character
;		tbladdr.ab	Address of 256-byte table
;		dstlen.rw	Length of destination string
;		dstaddr.ab	Address of destination string
;-

	.MACRO	MOVTC	srclen.rw , srcaddr.ab , -
			fill.rb , tbladdr.ab , -
			dstlen.rw , dstaddr.ab

	    .WARN ;This instruction is not in the Subset VAX-11 Emulator

	    MOVTC_FE	srclen.rw , srcaddr.ab , -
			fill.rb , tbladdr.ab , -
			dstlen.rw , dstaddr.ab

	.ENDM	MOVTC


;+
